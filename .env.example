# ═══════════════════════════════════════════════════════════════════
# PUMP.FUN ACCOUNT CREATOR CONFIGURATION
# Copy this file to .env and fill in your values
# ═══════════════════════════════════════════════════════════════════

# ─── Proxy Configuration ───
USE_PROXY=true
PROXY_URL=http://p.webshare.io:9999/

# ─── OTP Configuration ───
OTP_API_URL=https://email.famecheap.support/webmail/pumpfun/INBOX
OTP_TIMEOUT=90

# ─── Follow Configuration ───
DO_FOLLOW=true
MAIN_PROFILE=your_wallet_address

# ─── Reply Configuration ───
SEND_REPLY=false
RANDOM_REPLY=true
REPLY_MESSAGE=your_message
REPLY_COIN=coin_address

# ─── Profile Update Configuration ───
UPDATE_PROFILE=true
PROFILE_BIO=your_bio
INTERNET_IMAGE=true

# ─── Database Configuration ───
SAVE_DATABASE=true
DB_HOST=your_db_host
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name
DB_TABLE=pumpfun

# ─── Multi-threading Configuration ───
MAX_CONCURRENCY=5

# ─── Display Configuration ───
PROGRESS_BAR=true
